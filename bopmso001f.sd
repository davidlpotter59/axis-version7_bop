  Writefile(L_FILE_NAME,"%s%s%n%s%s%s%n%s%s%s",L_ARIAL,"<033>&a",
                        L_BASIC_POS+250,"h+0V","Limit:",
                        "<033>&a",L_DOLLAR_POS,
                        "h+0V",L_ARIAL_BOLD,"$")                  --Basic Limit

  Writefile(L_FILE_NAME,"%s%s%n%s%s",l_letter_gothic_bold_10,"<033>&a",L_LIMIT_POS,"h+0V",
                        Str(bpsdefault2:SUPPLIES_LIMIT,"ZZZ,ZZZ,ZZ9"))
  Writefile(L_FILE_NAME,"%s","<NL>")


--  L_LIMIT            = BOPGENERAL:PROPERTY_LIMIT * (bpsdefault2:AR_PERCENT Divide 100)
--  L_REVISED_LIMIT    = BOPGENERAL:AR_LIMIT
--  l_bpsdefault_limit = bpsdefault2:AR_LIMIT

--  Do PRINT_LIMIT

  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
  Writefile(L_FILE_NAME,"%s","<033>&a")
