Program
  Name = "bopmso001f";
  Type = "File";
  Domain = "SFPPRINT1";
  Padcharacter = Space;

 String L_FILE_NAME[13] = "bopmso001f.cq";
 Unsigned Ascii Number L_PAGE_NUMBER = 1;
