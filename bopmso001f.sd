  Writefile(L_FILE_NAME,"%s%s%n%s%s%s",L_ARIAL_BOLD,"<033>&a",
                        L_COVERGE_POS,"h+0V","Buildings","<NL>")


  Writefile(L_FILE_NAME,"%s%s%n%s%s%s%s",L_ARIAL_BOLD,"<033>&a",
                        L_COVERGE_POS,"h+0V","F.     ",L_ARIAL,
