    L_LIMIT            = 0 --resets L_LIMIT
    Do PRINT_LIMIT

    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
    Writefile(L_FILE_NAME,"%s","<033>&a")
    Writefile(L_FILE_NAME,"%n",L_COVERGE_POS)
    Writefile(L_FILE_NAME,"%s","h+0V")
    Writefile(L_FILE_NAME,"%s","Personal Injury /Advertising Injury (Pg. 17)")
    Writefile(L_FILE_NAME,"%s",L_ARIAL)
    Writefile(L_FILE_NAME,"%s","<033>&a")
    Writefile(L_FILE_NAME,"%n",L_BASIC_POS)
    Writefile(L_FILE_NAME,"%s","h+0V")
    Writefile(L_FILE_NAME,"%s","Basic Limit:")
    Writefile(L_FILE_NAME,"%s","<033>&a")
    Writefile(L_FILE_NAME,"%n",L_DOLLAR_POS)
    Writefile(L_FILE_NAME,"%s","h+0V")
    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)

    Writefile(L_FILE_NAME,"%s","$")
    Writefile(L_FILE_NAME,"%s","<033>&f0S") --pushes current cursor onto stack
    Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
    Writefile(L_FILE_NAME,"%s","<033>&a")
    Writefile(L_FILE_NAME,"%n",L_LIMIT_POS+700)
    Writefile(L_FILE_NAME,"%s","h+0V")
    Writefile(L_FILE_NAME,"%s",L_ARIAL)
    Writefile(L_FILE_NAME,"%s","< Limit - All Locations")
    Writefile(L_FILE_NAME,"%s","<NL>")

    If SCIPSCONTROL:PRINT_REVISED_ON_SUPP = 1 Then
      {
        Writefile(L_FILE_NAME,"%s",L_ARIAL)
        Writefile(L_FILE_NAME,"%s","<033>&a")
        Writefile(L_FILE_NAME,"%n",L_REVISED_POS)
        Writefile(L_FILE_NAME,"%s","h+0V")
        Writefile(L_FILE_NAME,"%s","Revised Limit:")
        Writefile(L_FILE_NAME,"%s","<033>&a")
        Writefile(L_FILE_NAME,"%n",L_DOLLAR_POS)
        Writefile(L_FILE_NAME,"%s","h+0V")
        Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
        Writefile(L_FILE_NAME,"%s","$")
        Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
        Writefile(L_FILE_NAME,"%s","<033>&a")
        Writefile(L_FILE_NAME,"%n",L_LIMIT_POS+700)
        Writefile(L_FILE_NAME,"%s","h+0V")
        Writefile(L_FILE_NAME,"%s",L_ARIAL)
        Writefile(L_FILE_NAME,"%s","< Limit - All Locations")
        Writefile(L_FILE_NAME,"%s","<NL>")
      }

      Writefile(L_FILE_NAME,"%s","<033>&f1S")--pop to return to basic limit

    Access SFPPRINT3,
       Set SFPPRINT3:POLICY_NO    = SFPPRINT1:POLICY_NO,
           SFPPRINT3:POL_YEAR     = SFPPRINT1:POL_YEAR,
           SFPPRINT3:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic

    L_FMA100 = 0
    While SFPPRINT3:POLICY_NO    = SFPPRINT1:POLICY_NO And
          SFPPRINT3:POL_YEAR     = SFPPRINT1:POL_YEAR And
          SFPPRINT3:END_SEQUENCE = SFPPRINT1:END_SEQUENCE
      {
        If SFPPRINT3:CODE = "FMA100 " Then
          {
            L_FMA100 = 1
          }
        Next SFPPRINT3
      }
    If L_FMA100 = 1 Then  --endorsement deletes coverage for personal injury
      {
        Writefile(L_FILE_NAME,"%s","<033>&a")
        Writefile(L_FILE_NAME,"%n",L_DOLLAR_POS)
        Writefile(L_FILE_NAME,"%s","h+0V")
        Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
        Writefile(L_FILE_NAME,"%s","$")
        Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
        Writefile(L_FILE_NAME,"%s","<033>&a")
        Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
        Writefile(L_FILE_NAME,"%s","h+0V")
        Writefile(L_FILE_NAME,"%s",Str(0,"ZZZ,ZZZ,ZZ9"))
        If SCIPSCONTROL:PRINT_REVISED_ON_SUPP = 1 Then
          {
            Writefile(L_FILE_NAME,"%s",L_ARIAL)
            Writefile(L_FILE_NAME,"%s","<033>&a")
            Writefile(L_FILE_NAME,"%n",L_REVISED_POS)
            Writefile(L_FILE_NAME,"%s","h+0V")
            Writefile(L_FILE_NAME,"%s","Revised Limit:")
            Writefile(L_FILE_NAME,"%s","<033>&a")
            Writefile(L_FILE_NAME,"%n",L_DOLLAR_POS)
            Writefile(L_FILE_NAME,"%s","h+0V")
            Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
            Writefile(L_FILE_NAME,"%s","$")
          }
      }
   Else
     {
       L_REVISED_LIMIT    = SFPMASTER:PERSONAL_ADD_INJURY
       L_BPSDEFAULT_LIMIT = BPSDEFAULT:PERSONAL_ADD_INJURY
       L_LIMIT            = 0
       Do PRINT_LIMIT
     }

                                    -- PAGE 3

    Writefile(L_FILE_NAME,"%s",L_ARIAL)
    Writefile(L_FILE_NAME,"%s","<033>&a")
    Writefile(L_FILE_NAME,"%n",L_COVERGE_POS+400)
    Writefile(L_FILE_NAME,"%s","h7250V")
    Writefile(L_FILE_NAME,"%s",L_COPYRIGHT_8)
    Writefile(L_FILE_NAME,"%s",L_ARIAL)
    Writefile(L_FILE_NAME,"%s"," 2007 MSO, Inc")
    Writefile(L_FILE_NAME,"%s","<FF>")

    L_PAGE_NUMBER = L_PAGE_NUMBER + 1
    --border
    If SCIPSCONTROL:PRINT_BORDERS_ON_DEC_PAGE = 1 Then
      {
        Writefile(L_FILE_NAME,"%s","<033>&a140h-220V<033>*c10a3100b0P")   --left vertical li
        Writefile(L_FILE_NAME,"%s","<033>&a5585h-0V<033>*c10a3100b0P")   --right vertical
        Writefile(L_FILE_NAME,"%s","<033>&a140h-0V<033>*c5467h20v0P") --top horizontal line to
        Writefile(L_FILE_NAME,"%s","<033>&a140h7300V<033>*c5467h20v0P") --bottom horizontal line
      }
    Writefile(L_FILE_NAME,"%s", L_ARIAL_BOLD_9)
    Writefile(L_FILE_NAME,"%s","<033>&a5000h7250V")
    Writefile(L_FILE_NAME,"%s","Page ")
    Writefile(L_FILE_NAME,"%n",L_PAGE_NUMBER)

    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_12)
    Writefile(L_FILE_NAME,"%s","<033>&a180h0V")
    Writefile(L_FILE_NAME,"%s","DECLARATION SUPPLEMENT - ")
    Writefile(L_FILE_NAME,"%s",SFSSTATE:DESCRIPTION)
    Writefile(L_FILE_NAME,"%s",L_ARIAL)
    Writefile(L_FILE_NAME,"%s","<033>&a4750h0V")
    Writefile(L_FILE_NAME,"%s",Trun(SFPPRINT4:FORM_PRINT))
    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
    Writefile(L_FILE_NAME,"%s"," ")
    Writefile(L_FILE_NAME,"%s",SFPPRINT4:EDITION_PRINT)
    Writefile(L_FILE_NAME,"%s","<033>&a140h+60V<033>*c5467h5v0P")

    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
    Writefile(L_FILE_NAME,"%s","<033>&a180h+240V")
    Writefile(L_FILE_NAME,"%s","Policy Number: ")
    Writefile(L_FILE_NAME,"%s",Trun(SFSLINE:ALPHA))

    Writefile(L_FILE_NAME,"%n",SFPPRINT1:POLICY_NO)
    Writefile(L_FILE_NAME,"%s","<033>&a3275h+120V")
    Writefile(L_FILE_NAME,"%s","Effective:")
    Writefile(L_FILE_NAME,"%s","<033>&a3750h+0V")
    Writefile(L_FILE_NAME,"%d",SFPNAME:EFF_DATE)

    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
    Writefile(L_FILE_NAME,"%s","<033>&a")
    Writefile(L_FILE_NAME,"%n",L_COVERGE_POS)
    Writefile(L_FILE_NAME,"%s","h+240V")
    Writefile(L_FILE_NAME,"%s","Supplemental - Continued")
    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
    Writefile(L_FILE_NAME,"%s","<033>&a2300h+0V")
    Writefile(L_FILE_NAME,"%s","Location")
    Writefile(L_FILE_NAME,"%s","<033>&a3000h+0V")
    Writefile(L_FILE_NAME,"%n",SFPLOCATION:PREM_NO)
    Writefile(L_FILE_NAME,"%s","<033>&a3300h+0V")
    Writefile(L_FILE_NAME,"%s","Building")
    Writefile(L_FILE_NAME,"%s","<033>&a3900h+0V")
    Writefile(L_FILE_NAME,"%n",SFPLOCATION:BUILD_NO)

    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
    Writefile(L_FILE_NAME,"%s","<033>&a")
    Writefile(L_FILE_NAME,"%n",L_COVERGE_POS)
    Writefile(L_FILE_NAME,"%s","h+240V")
    Writefile(L_FILE_NAME,"%s","OTHER SPECIAL POLICY CONDITIONS:")
    Writefile(L_FILE_NAME,"%s","<nl>")
    Writefile(L_FILE_NAME,"%s","<nl>")

  /*--------------------------------------------------------------------------
   The following code is for non-manditory endorsements
   ---------------------------------------------------------------------------*/
    Access SFPPRINT3,
       Set SFPPRINT3:POLICY_NO    = SFPPRINT1:POLICY_NO,
           SFPPRINT3:POL_YEAR     = SFPPRINT1:POL_YEAR,
           SFPPRINT3:END_SEQUENCE = SFPPRINT1:END_SEQUENCE,
           sfpprint3:prem_no      = sfplocation:prem_no,
           sfpprint3:build_no     = sfplocation:build_no, Generic

    While SFPPRINT3:POLICY_NO    = SFPPRINT1:POLICY_NO And
          SFPPRINT3:POL_YEAR     = SFPPRINT1:POL_YEAR And
          SFPPRINT3:END_SEQUENCE = SFPPRINT1:END_SEQUENCE and
          sfpprint3:prem_no      = sfplocation:prem_no and
          sfpprint3:build_no     = sfplocation:build_no
      {
        If L_LINE_PCOUNTER >= L_MAX_LINES - 6 Then
          {
            L_PAGE_NUMBER = L_PAGE_NUMBER +1
            Writefile(L_FILE_NAME,"%s","<FF>")
            Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
            Writefile(L_FILE_NAME,"%s","<033>&a5000h7250V")
            Writefile(L_FILE_NAME,"%s","Page ")
            Writefile(L_FILE_NAME,"%n",L_PAGE_NUMBER)
            L_LINE_PCOUNTER = 1
           --border
            If SCIPSCONTROL:PRINT_BORDERS_ON_DEC_PAGE = 1 Then
              {
                Writefile(L_FILE_NAME,"%s","<033>&a140h0V<033>*c10a3045b0P")   --left vertical li
                Writefile(L_FILE_NAME,"%s","<033>&a5585h0V<033>*c10a3045b0P")   --right vertical
                Writefile(L_FILE_NAME,"%s","<033>&a140h0V<033>*c5467h20v0P") --top horizontal line to
                Writefile(L_FILE_NAME,"%s","<033>&a140h7300V<033>*c5467h20v0P") --bottom horizontal line
              }
            Writefile(L_FILE_NAME,"%s","<033>&a+0h0V")
            Writefile(L_FILE_NAME,"%s","<NL>") --sets starting point for newpage
            Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
            Writefile(L_FILE_NAME,"%s","<033>&a1800h+0V")
            Writefile(L_FILE_NAME,"%s","FORMS AND ENDORSEMENTS CONTINUED")
            Writefile(L_FILE_NAME,"%s","<NL>")
            L_LINE_PCOUNTER = L_LINE_PCOUNTER +1

            Writefile(L_FILE_NAME,"%s","<033>&a150h-80V<033>*c5455h20v0P")
            Writefile(L_FILE_NAME,"%s","<NL>")
            Writefile(L_FILE_NAME,"%s","<033>&a+0h+20V")
            L_LINE_PCOUNTER = L_LINE_PCOUNTER +1
          }

        If SFPPRINT3:PRINT_FORM = "Y" Then
          {
            Writefile(L_FILE_NAME,"%s",L_ARIAL)
            Writefile(L_FILE_NAME,"%s","<033>&a1000h+0V")
            Writefile(L_FILE_NAME,"%s",SFPPRINT3:FORM_EDITION)
            Writefile(L_FILE_NAME,"%s","<033>&a2400h+0V")
            Writefile(L_FILE_NAME,"%s",SFPPRINT3:DESCRIPTION)
            Writefile(L_FILE_NAME,"%s","<NL>")
            L_LINE_PCOUNTER = L_LINE_PCOUNTER + 1
          }
        Next SFPPRINT3
      }
    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
    Writefile(L_FILE_NAME,"%s","<033>&a")
    Writefile(L_FILE_NAME,"%n",L_COVERGE_POS)
    Writefile(L_FILE_NAME,"%s","h+0V")
    Writefile(L_FILE_NAME,"%s","IMPORTANT MESSAGES")

    Writefile(L_FILE_NAME,"%s",L_ARIAL)
    Writefile(L_FILE_NAME,"%s","<033>&a")
    Writefile(L_FILE_NAME,"%n",L_COVERGE_POS+400)
    Writefile(L_FILE_NAME,"%s","h7250V")
    Writefile(L_FILE_NAME,"%s",L_COPYRIGHT_8)
    Writefile(L_FILE_NAME,"%s",L_ARIAL)
    Writefile(L_FILE_NAME,"%s"," 2007 MSO, Inc")
    Writefile(L_FILE_NAME,"%s","<FF>")

    If L_PAGE_NUMBER = 3 Then
      {
        Writefile(L_FILE_NAME,"%s","<FF>")
      }
  Next SFPLOCATION
}
End
end
